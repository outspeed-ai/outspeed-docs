{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Outspeed Docs",
  "colors": {
    "primary": "#009689",
    "light": "#00BBA7",
    "dark": "#009689"
  },
  "fonts": {
    "heading": {
      "family": "Montserrat"
    }
  },
  "favicon": "/logo/outspeed-flat-inverse-favicon.svg",
  "navigation": {
    "tabs": [
      {
        "tab": "Welcome",
        "pages": ["get-started/overview"]
      },
      {
        "tab": "Developer Guide",
        "groups": [
          {
            "group": "Get Started",
            "pages": ["get-started/quickstart", "get-started/templates"]
          },
          {
            "group": "Tools",
            "pages": [
              "features/tool-use",
              "features/client-tools",
              "features/system-tools",
              "features/skip-turn",
              "features/end-session"
            ]
          },
          {
            "group": "Controls",
            "pages": [
              "features/speak-first",
              "features/context-management",
              "features/voices",
              "features/send-text",
              "features/speak",
              "features/pronunciation",
              "features/resume-session",
              "features/input-language"
            ]
          }
        ]
      },
      {
        "tab": "SDKs",
        "groups": [
          {
            "group": "Web",
            "pages": ["react/setup", "react/example"]
          },
          {
            "group": "iOS",
            "pages": ["iOS/outspeed-swift-getting-started", "iOS/outspeed-swift-elevenlabs"]
          }
        ]
      },
      {
        "tab": "API",
        "groups": [
          {
            "group": "API Reference",
            "pages": ["api-spec/live-api", "api-spec/client", "api-spec/server", "api-spec/tts"]
          },
          {
            "group": "Compatibility",
            "pages": ["get-started/openai-compatibility"]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Discord Community",
          "href": "https://discord.gg/cmfFw6SYvp",
          "icon": "discord"
        },
        {
          "anchor": "Book a Demo",
          "href": "https://cal.com/outspeed/early-access",
          "icon": "phone"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/2.svg",
    "dark": "/logo/3.svg"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "https://discord.gg/cmfFw6SYvp"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Get API Key",
      "href": "https://cal.com/outspeed/early-access"
    }
  },
  "footer": {
    "socials": {
      "discord": "https://discord.gg/cmfFw6SYvp",
      "github": "https://github.com/outspeed-ai/outspeed-examples"
    }
  }
}
